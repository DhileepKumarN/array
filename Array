#include <iostream>
using namespace std;
int main()
{
  int i,n;
  cout<<"enter number of elements in array"<<endl;
  cin>>n;
  int array[n];
  cout<<"enter elements in array"<<endl;
  for(i=0;i<n;i++)
  {
    cin>>array[i];
  }
  cout<<"entered elements in array are"<<endl;
  for(i=0;i<n;i++)
  {
    cout<<array[i]<<endl;
  }
  return 0;
}
